<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginService</servlet-name>
    <servlet-class>com.miaoingservice.user.LoginService</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginService</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
    <servlet>
        <servlet-name>RegisterService</servlet-name>
        <servlet-class>com.miaoingservice.user.RegisterService</servlet-class>
    </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getImage</servlet-name>
    <servlet-class>com.miaoxingservice.pic.getImage</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ImageService</servlet-name>
    <servlet-class>com.miaoxingservice.pic.ImageService</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MatchingDataService</servlet-name>
    <servlet-class>com.miaoingservice.matching.MatchingDataService</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SharingDataService</servlet-name>
    <servlet-class>com.miaoingservice.sharing.SharingDataService</servlet-class>
  </servlet>




    <servlet-mapping>
        <servlet-name>RegisterService</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getImage</servlet-name>
    <url-pattern>/getImage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ImageService</servlet-name>
    <url-pattern>/servlet/ImageService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MatchingDataService</servlet-name>
    <url-pattern>/fetchmatchingdata</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SharingDataService</servlet-name>
    <url-pattern>/fetchsharingdata</url-pattern>
  </servlet-mapping>
</web-app>
